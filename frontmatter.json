{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Content preview",
          "name": "preview",
          "type": "image"
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "layout",
          "name": "layout",
          "type": "string"
        },
        {
          "title": "last_modified_at",
          "name": "last_modified_at",
          "type": "datetime"
        },
        {
          "title": "permalink",
          "name": "permalink",
          "type": "string"
        },
        {
          "title": "published",
          "name": "published",
          "type": "boolean"
        },
        {
          "title": "comments",
          "name": "comments",
          "type": "boolean"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "jekyll",
  "frontMatter.content.publicFolder": "assets",
  "frontMatter.preview.host": "http://localhost:4000",
  "frontMatter.content.pageFolders": [
    {
      "title": "drafts",
      "path": "[[workspace]]/_drafts"
    },
    {
      "title": "posts",
      "path": "[[workspace]]/_posts"
    },
    {
      "title": "_pages",
      "path": "[[workspace]]/_pages"
    }
  ],
  "frontMatter.content.snippets": {
    "Post Frontmatter": {
      "description": "Post frontmatter",
      "body": [
        "---",
        "title: \"\"",
        "date: ",
        "categories: Essays",
        "tags: ",
        "permalink: /",
        "published: true",
        "comments: true",
        "toc: true",
        "excerpt: \"\"",
        "---"
      ],
      "fields": []
    },
    "Header image": {
      "description": "",
      "body": [
        "teaser: /assets/images/articles/steve-irwin.webp",
        "header:",
        "  image: /assets/images/articles/steve-irwin.webp"
      ],
      "isMediaSnippet": true
    },
    "Image with link": {
      "description": "",
      "body": "[![Hiring Virtual Workers For Small Business on Amazon Kindle](/assets/images/books/Hiring-Virtual-Workers.webp)](https://amzn.to/2FvAxx9)",
      "isMediaSnippet": true
    },
    "Link": {
      "description": "",
      "body": "[Hiring Virtual Workers For Small Business Book on Amazon Kindle](https://amzn.to/2FvAxx9)",
      "isMediaSnippet": true
    }
  },
  "frontMatter.git.enabled": true,
  "frontMatter.dashboard.openOnStart": true
}